#quiz
  = form_for @quiz, :url => { :action => "create" } do |f|
    %br Question
    = f.text_area "question"
    %br Answer 1
    = f.text_area "option1"
    %br Answer 2
    = f.text_area "option2"
    %br Answer 3
    = f.text_area "option3"
    %br Answer 4
    = f.text_area "option4"
    %br
    = f.submit "publish"
